# Docker Environment Variables Example
# Copy this file to .env and fill in your actual values
# DO NOT commit .env with real credentials to version control

OPENAI_API_KEY=<OPENAI_API_KEY>
PINECONE_API_KEY=<PINECONE_API_KEY>
PINECONE_ENVIRONMENT=gcp-starter

# Keycloak OAuth Configuration for Chainlit
OAUTH_KEYCLOAK_CLIENT_ID=minio
OAUTH_KEYCLOAK_CLIENT_SECRET=<KEYCLOAK_CLIENT_SECRET>
OAUTH_KEYCLOAK_REALM=humaine
OAUTH_KEYCLOAK_BASE_URL=https://keycloak.humaine-horizon.eu
OAUTH_KEYCLOAK_NAME=keycloak

# MinIO Configuration
MINIO_SECURE=true
MINIO_ENDPOINT=minio.humaine-horizon.eu

# Kubeflow Configuration 
KUBEFLOW_HOST=https://kubeflow.humaine-horizon.eu/pipeline

